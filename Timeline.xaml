<UserControl x:Class="TimelineCreator.Timeline"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             
             Loaded="UserControl_Loaded" SizeChanged="UserControl_SizeChanged" MouseWheel="UserControl_MouseWheel" MouseDown="UserControl_MouseDown" MouseUp="UserControl_MouseUp" MouseMove="UserControl_MouseMove" Background="White">

    <Grid x:Name="theGrid" Background="White" HorizontalAlignment="Stretch" SnapsToDevicePixels="True"/>
</UserControl>
